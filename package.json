  "homepage": "https://github.com/Khan/aphrodite",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "chai": "^3.3.0",
    "eslint": "^3.7.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-react": "^6.3.0",
    "jsdom": "^6.5.1",
    "mocha": "^2.3.3",
    "npm-run-all": "^1.7.0",
    "nyc": "^6.4.4",
    "rimraf": "^2.5.2",
